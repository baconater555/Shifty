local Tab = Window:CreateTab("4yr olds!", 4483362458) -- Title, Image
local Section = MainTab:CreateSection("Section of 1yr olds")

local Button = MainTab:CreateButton({
   Name = "Baby shat(lag)",
   Callback = function()
   local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local FreezeEvent = ReplicatedStorage:WaitForChild("FreezeEvent")
local RunService = game:GetService("RunService")

FreezeEvent.OnClientEvent:Connect(function()
    local character = player.Character
    if not character then return end
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    local camera = workspace.CurrentCamera

    if humanoid and camera then
        local originalWalkSpeed = humanoid.WalkSpeed
        local originalJumpPower = humanoid.JumpPower

        -- Freeze movement completely
        humanoid.WalkSpeed = 0
        humanoid.JumpPower = 0

        -- Slow-motion camera effect
        local originalCFrame = camera.CFrame
        local t = 0
        local duration = 4 -- stronger, lasts longer
        local lastCFrame = camera.CFrame

        local connection
        connection = RunService.RenderStepped:Connect(function(dt)
            t = t + dt
            if t > duration then
                connection:Disconnect()
                humanoid.WalkSpeed = originalWalkSpeed
                humanoid.JumpPower = originalJumpPower
                camera.CFrame = originalCFrame
            else
                -- Strong stutter effect
                local offsetX = math.sin(t*10)*0.3
                local offsetY = math.sin(t*12)*0.3
                camera.CFrame = lastCFrame:Lerp(camera.CFrame * CFrame.new(offsetX, offsetY, 0), 0.03)
                lastCFrame = camera.CFrame
            end
        end)
    end
end)
   end,
})

local Button = MainTab:CreateButton({
   Name = "pvp for (Sab)",
   Callback = function()
   local ReplicatedStorage = game:GetService("ReplicatedStorage")
local activateShield = ReplicatedStorage:WaitForChild("ActivateShield")

activateShield.OnServerEvent:Connect(function(player)
    local character = player.Character
    if not character then return end
    
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then return end

    -- Prevent double-activation
    if player:GetAttribute("ShieldActive") then return end

    player:SetAttribute("ShieldActive", true)
    print(player.Name .. " has activated a shield!")

    -- Store original health changed event behavior
    local connection
    connection = humanoid.HealthChanged:Connect(function(newHealth)
        if player:GetAttribute("ShieldActive") == true then
            humanoid.Health = humanoid.MaxHealth  -- Keep full health
        end
    end)

    -- Shield lasts 5 seconds
    task.wait(5)

    -- Disable shield
    player:SetAttribute("ShieldActive", false)
    print(player.Name .. "'s shield ended.")

    -- Allow damage normally again
    if connection then
        connection:Disconnect()
    end
end)
   end,
})
